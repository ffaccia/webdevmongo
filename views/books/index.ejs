<h2>Seach Books</h2>
<form action="/books" method="GET">
    <div>
        <label for="title">Title</label>
        <input type="text" name="title" value="<%= searchOptions.title %>">
    </div>
    
    <div>
        <label for="publishedAfter">Published After</label>
        <input type="date" name="publishedAfter" value="<%= searchOptions.publishedAfter %>">
    </div>
    
    <div>
        <label for="publishedBefore">Published Before</label>
        <input type="date" name="publishedBefore" value="<%= searchOptions.publishedBefore %>">
    </div>
    
    <div>
        <button type="submit">Submit</button>
    </div>
</form>

<br>

<ul>
<% books.forEach( book => { %>
    <li>
        <div><%= book.title %></div>  
        <img width="100" height="150" src="<%= book.coverImagePath %>">  
    </li>    
<% }) %>
</ul>




